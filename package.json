  "bugs": {
    "url": "https://github.com/nicocube/wool-stream/issues"
  },
  "homepage": "https://github.com/nicocube/wool-stream#readme",
  "devDependencies": {
    "eslint": "^5.9.0",
    "nodemon": "^1.18.7",
    "nyc": "^13.1.0",
    "tap-summary": "^4.0.0",
    "tape": "^4.9.1",
    "tape-async": "^2.3.0"
  },
  "dependencies": {}
